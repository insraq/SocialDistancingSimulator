{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore",
            "allow-trailing-underscore"
        ],
        "object-literal-sort-keys": false,
        "no-namespace": [
            true,
            "allow-declarations"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "space-before-function-paren": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}